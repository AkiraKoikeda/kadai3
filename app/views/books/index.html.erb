<%= render 'list', book: @book %>

<h1>Books</h1>
<table>
<thead>
<tr>
    <th>Title</th>
    <th>Opinion</th>
</tr>
</thead>

<tbody>
<% @books.each do |book| %>

<tr>
 <td>
  <%= link_to book_path(book.id) do %>
   <%= book.book_name %>
  <% end %>
 </td>

 <td><%= book.caption %></td>

  <% end %>
 </td>
</tbody>
</table>
