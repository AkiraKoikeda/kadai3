<div class="container">
  <div class="row">
    <div class="col-lg-3">
<%= render 'books/list', book: @book %>
    </div>

    <div class="col-lg-8">
<h1>Users</h1>
<table>
<thead>
<tr>
    <th>image</th>
    <th>name</th>
</tr>
</thead>

<tbody>
<% @users.each do |user| %>

<tr>
 <td><%= image_tag user.get_profile_image(100,100) %></td>
 <td><%= user.name %></td>
 <td><%= link_to "Show", user_path(user.id) %></td>
<% end %>
</tbody>
</table>
    </div>
  </div>
</div>
